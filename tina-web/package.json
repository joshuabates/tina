    "clsx": "^2.1.1",
    "convex": "^1.31.7",
    "effect": "^3.19.16",
    "lucide-react": "^0.563.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
